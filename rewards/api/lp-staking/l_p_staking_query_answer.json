{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LPStakingQueryAnswer",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "token_info"
      ],
      "properties": {
        "token_info": {
          "type": "object",
          "required": [
            "decimals",
            "name",
            "symbol"
          ],
          "properties": {
            "decimals": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            },
            "name": {
              "type": "string"
            },
            "symbol": {
              "type": "string"
            },
            "total_supply": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Uint128"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "rewards"
      ],
      "properties": {
        "rewards": {
          "type": "object",
          "required": [
            "rewards"
          ],
          "properties": {
            "rewards": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "balance"
      ],
      "properties": {
        "balance": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "contract_status"
      ],
      "properties": {
        "contract_status": {
          "type": "object",
          "required": [
            "is_stopped"
          ],
          "properties": {
            "is_stopped": {
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "reward_token"
      ],
      "properties": {
        "reward_token": {
          "type": "object",
          "required": [
            "token"
          ],
          "properties": {
            "token": {
              "$ref": "#/definitions/ContractInfo"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "incentivized_token"
      ],
      "properties": {
        "incentivized_token": {
          "type": "object",
          "required": [
            "token"
          ],
          "properties": {
            "token": {
              "$ref": "#/definitions/ContractInfo"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "query_error"
      ],
      "properties": {
        "query_error": {
          "type": "object",
          "required": [
            "msg"
          ],
          "properties": {
            "msg": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "ContractInfo": {
      "description": "Code hash and address of a contract.",
      "type": "object",
      "required": [
        "address",
        "code_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "code_hash": {
          "type": "string"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
