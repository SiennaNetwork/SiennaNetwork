{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "lp_token_contract",
    "pair_contract"
  ],
  "properties": {
    "lp_token_contract": {
      "$ref": "#/definitions/ContractInstantiationInfo"
    },
    "pair_contract": {
      "$ref": "#/definitions/ContractInstantiationInfo"
    }
  },
  "definitions": {
    "ContractInstantiationInfo": {
      "description": "Info used to instantiate a contract",
      "type": "object",
      "required": [
        "code_hash",
        "id"
      ],
      "properties": {
        "code_hash": {
          "type": "string"
        },
        "id": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    }
  }
}
