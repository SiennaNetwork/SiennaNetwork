{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Response",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "status"
      ],
      "properties": {
        "status": {
          "type": "object",
          "required": [
            "errors"
          ],
          "properties": {
            "errors": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "launched": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "schedule"
      ],
      "properties": {
        "schedule": {
          "type": "object",
          "required": [
            "schedule",
            "total"
          ],
          "properties": {
            "schedule": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Portion"
              }
            },
            "total": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Portion": {
      "description": "Claimable portion",
      "type": "object",
      "required": [
        "address",
        "amount",
        "reason",
        "vested"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "reason": {
          "type": "string"
        },
        "vested": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
