{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Handle",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "auth"
      ],
      "properties": {
        "auth": {
          "$ref": "#/definitions/AuthHandle"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "create_viewing_key"
      ],
      "properties": {
        "create_viewing_key": {
          "type": "object",
          "required": [
            "entropy"
          ],
          "properties": {
            "entropy": {
              "type": "string"
            },
            "padding": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "set_viewing_key"
      ],
      "properties": {
        "set_viewing_key": {
          "type": "object",
          "required": [
            "key"
          ],
          "properties": {
            "key": {
              "type": "string"
            },
            "padding": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "immigration"
      ],
      "properties": {
        "immigration": {
          "$ref": "#/definitions/ImmigrationHandle"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "emigration"
      ],
      "properties": {
        "emigration": {
          "$ref": "#/definitions/EmigrationHandle"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "rewards"
      ],
      "properties": {
        "rewards": {
          "$ref": "#/definitions/RewardsHandle"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "drain"
      ],
      "properties": {
        "drain": {
          "type": "object",
          "required": [
            "key",
            "snip20"
          ],
          "properties": {
            "key": {
              "type": "string"
            },
            "recipient": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "snip20": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "AuthHandle": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "change_admin"
          ],
          "properties": {
            "change_admin": {
              "type": "object",
              "required": [
                "address"
              ],
              "properties": {
                "address": {
                  "$ref": "#/definitions/HumanAddr"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "create_viewing_key"
          ],
          "properties": {
            "create_viewing_key": {
              "type": "object",
              "required": [
                "entropy"
              ],
              "properties": {
                "entropy": {
                  "type": "string"
                },
                "padding": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "set_viewing_key"
          ],
          "properties": {
            "set_viewing_key": {
              "type": "object",
              "required": [
                "key"
              ],
              "properties": {
                "key": {
                  "type": "string"
                },
                "padding": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              }
            }
          }
        }
      ]
    },
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "ContractLink_for_HumanAddr": {
      "description": "Info needed to talk to a contract instance.",
      "type": "object",
      "required": [
        "address",
        "code_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "code_hash": {
          "type": "string"
        }
      }
    },
    "EmigrationHandle": {
      "anyOf": [
        {
          "description": "Allow another contract to receive data from this contract",
          "type": "object",
          "required": [
            "enable_migration_to"
          ],
          "properties": {
            "enable_migration_to": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        },
        {
          "description": "Disallow another contract to receive data from this contract",
          "type": "object",
          "required": [
            "disable_migration_to"
          ],
          "properties": {
            "disable_migration_to": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        },
        {
          "description": "Export migration data to another contract. Must be called by a contract migration to which was enabled via `EnableMigrationTo`, and pass an address for which the migration is to be performed.",
          "type": "object",
          "required": [
            "export_state"
          ],
          "properties": {
            "export_state": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    },
    "ImmigrationHandle": {
      "anyOf": [
        {
          "description": "Allow this contract to receive data from another contract",
          "type": "object",
          "required": [
            "enable_migration_from"
          ],
          "properties": {
            "enable_migration_from": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        },
        {
          "description": "Disallow this contract to receive data from another contract",
          "type": "object",
          "required": [
            "disable_migration_from"
          ],
          "properties": {
            "disable_migration_from": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        },
        {
          "description": "Request migration data from another contract. Called by the user to initiate a migration.",
          "type": "object",
          "required": [
            "request_migration"
          ],
          "properties": {
            "request_migration": {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            }
          }
        },
        {
          "description": "Callback containing migration data. Must be called by a contract migration from which was enabled via `EnableMigrationFrom`.",
          "type": "object",
          "required": [
            "receive_migration"
          ],
          "properties": {
            "receive_migration": {
              "$ref": "#/definitions/Binary"
            }
          }
        }
      ]
    },
    "RewardsConfig": {
      "description": "Reward pool configuration",
      "type": "object",
      "properties": {
        "bonding": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "lp_token": {
          "anyOf": [
            {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            },
            {
              "type": "null"
            }
          ]
        },
        "reward_token": {
          "anyOf": [
            {
              "$ref": "#/definitions/ContractLink_for_HumanAddr"
            },
            {
              "type": "null"
            }
          ]
        },
        "reward_vk": {
          "type": [
            "string",
            "null"
          ]
        },
        "timekeeper": {
          "anyOf": [
            {
              "$ref": "#/definitions/HumanAddr"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "RewardsHandle": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "deposit"
          ],
          "properties": {
            "deposit": {
              "type": "object",
              "required": [
                "amount"
              ],
              "properties": {
                "amount": {
                  "$ref": "#/definitions/Uint128"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "withdraw"
          ],
          "properties": {
            "withdraw": {
              "type": "object",
              "required": [
                "amount"
              ],
              "properties": {
                "amount": {
                  "$ref": "#/definitions/Uint128"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "claim"
          ],
          "properties": {
            "claim": {
              "type": "object"
            }
          }
        },
        {
          "type": "object",
          "required": [
            "begin_epoch"
          ],
          "properties": {
            "begin_epoch": {
              "type": "object",
              "required": [
                "next_epoch"
              ],
              "properties": {
                "next_epoch": {
                  "type": "integer",
                  "format": "uint64",
                  "minimum": 0.0
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "configure"
          ],
          "properties": {
            "configure": {
              "$ref": "#/definitions/RewardsConfig"
            }
          }
        },
        {
          "type": "object",
          "required": [
            "close"
          ],
          "properties": {
            "close": {
              "type": "object",
              "required": [
                "message"
              ],
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "Uint128": {
      "type": "string"
    }
  }
}
