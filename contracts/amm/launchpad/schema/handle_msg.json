{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "description": "Set pause/migration status",
      "type": "object",
      "required": [
        "set_status"
      ],
      "properties": {
        "set_status": {
          "type": "object",
          "required": [
            "level",
            "reason"
          ],
          "properties": {
            "level": {
              "$ref": "#/definitions/ContractStatusLevel"
            },
            "new_address": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "reason": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "description": "SNIP20 receiver interface",
      "type": "object",
      "required": [
        "receive"
      ],
      "properties": {
        "receive": {
          "type": "object",
          "required": [
            "amount",
            "from"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            },
            "from": {
              "$ref": "#/definitions/HumanAddr"
            },
            "msg": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Binary"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "description": "Lock for native token, amount set for locking will be floored to the closest segment, the rest will be sent back.",
      "type": "object",
      "required": [
        "lock"
      ],
      "properties": {
        "lock": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "description": "Perform unlocking of native token",
      "type": "object",
      "required": [
        "unlock"
      ],
      "properties": {
        "unlock": {
          "type": "object",
          "required": [
            "entries"
          ],
          "properties": {
            "entries": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "description": "Add additional token for locking into launchpad",
      "type": "object",
      "required": [
        "admin_add_token"
      ],
      "properties": {
        "admin_add_token": {
          "type": "object",
          "required": [
            "config"
          ],
          "properties": {
            "config": {
              "$ref": "#/definitions/TokenSettings"
            }
          }
        }
      }
    },
    {
      "description": "Remove token from the launchpad, this will send all the previously locked funds back to their owners",
      "type": "object",
      "required": [
        "admin_remove_token"
      ],
      "properties": {
        "admin_remove_token": {
          "type": "object",
          "required": [
            "index"
          ],
          "properties": {
            "index": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      }
    },
    {
      "description": "Change admin handle",
      "type": "object",
      "required": [
        "admin"
      ],
      "properties": {
        "admin": {
          "$ref": "#/definitions/HandleMsg"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "create_viewing_key"
      ],
      "properties": {
        "create_viewing_key": {
          "type": "object",
          "required": [
            "entropy"
          ],
          "properties": {
            "entropy": {
              "type": "string"
            },
            "padding": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "set_viewing_key"
      ],
      "properties": {
        "set_viewing_key": {
          "type": "object",
          "required": [
            "key"
          ],
          "properties": {
            "key": {
              "type": "string"
            },
            "padding": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "ContractStatusLevel": {
      "description": "Possible states of a contract.",
      "type": "string",
      "enum": [
        "Operational",
        "Paused",
        "Migrating"
      ]
    },
    "HandleMsg": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "change_admin"
          ],
          "properties": {
            "change_admin": {
              "type": "object",
              "required": [
                "address"
              ],
              "properties": {
                "address": {
                  "$ref": "#/definitions/HumanAddr"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "accept_admin"
          ],
          "properties": {
            "accept_admin": {
              "type": "object"
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    },
    "TokenSettings": {
      "description": "Configuration for single token that can be locked into the launchpad",
      "type": "object",
      "required": [
        "bounding_period",
        "segment",
        "token_type"
      ],
      "properties": {
        "bounding_period": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "segment": {
          "$ref": "#/definitions/Uint128"
        },
        "token_type": {
          "$ref": "#/definitions/TokenType"
        }
      }
    },
    "TokenType": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "custom_token"
          ],
          "properties": {
            "custom_token": {
              "type": "object",
              "required": [
                "contract_addr",
                "token_code_hash"
              ],
              "properties": {
                "contract_addr": {
                  "$ref": "#/definitions/HumanAddr"
                },
                "token_code_hash": {
                  "type": "string"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "native_token"
          ],
          "properties": {
            "native_token": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "Uint128": {
      "type": "string"
    }
  }
}
