{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "receive_swap_data"
      ],
      "properties": {
        "receive_swap_data": {
          "type": "object",
          "required": [
            "account",
            "asset_in",
            "asset_out"
          ],
          "properties": {
            "account": {
              "$ref": "#/definitions/HumanAddr"
            },
            "asset_in": {
              "$ref": "#/definitions/Asset"
            },
            "asset_out": {
              "$ref": "#/definitions/Asset"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "add_pairs"
      ],
      "properties": {
        "add_pairs": {
          "type": "object",
          "required": [
            "pairs"
          ],
          "properties": {
            "pairs": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Pair"
              }
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "remove_pairs"
      ],
      "properties": {
        "remove_pairs": {
          "type": "object",
          "required": [
            "pairs"
          ],
          "properties": {
            "pairs": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Pair"
              }
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "set_admin"
      ],
      "properties": {
        "set_admin": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "set_data_sender"
      ],
      "properties": {
        "set_data_sender": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Asset": {
      "type": "object",
      "required": [
        "amount",
        "info"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "info": {
          "$ref": "#/definitions/AssetInfo"
        }
      }
    },
    "AssetInfo": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "token"
          ],
          "properties": {
            "token": {
              "type": "object",
              "required": [
                "contract_addr",
                "token_code_hash",
                "viewing_key"
              ],
              "properties": {
                "contract_addr": {
                  "$ref": "#/definitions/HumanAddr"
                },
                "token_code_hash": {
                  "type": "string"
                },
                "viewing_key": {
                  "type": "string"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "native_token"
          ],
          "properties": {
            "native_token": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    },
    "Pair": {
      "type": "object",
      "required": [
        "asset_0",
        "asset_1"
      ],
      "properties": {
        "asset_0": {
          "$ref": "#/definitions/HumanAddr"
        },
        "asset_1": {
          "$ref": "#/definitions/HumanAddr"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
