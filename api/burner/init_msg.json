{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "burn_pool",
    "factory_address",
    "sienna_token"
  ],
  "properties": {
    "admins": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    },
    "burn_pool": {
      "description": "The account to burn SIENNA from",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "factory_address": {
      "description": "Needs to be added as an admin in order to allow it to add new pair addresses.",
      "allOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        }
      ]
    },
    "pairs": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    },
    "sienna_token": {
      "description": "SIENNA token",
      "allOf": [
        {
          "$ref": "#/definitions/ContractInstance_for_HumanAddr"
        }
      ]
    }
  },
  "definitions": {
    "ContractInstance_for_HumanAddr": {
      "description": "Info needed to talk to a contract instance.",
      "type": "object",
      "required": [
        "address",
        "code_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "code_hash": {
          "type": "string"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
