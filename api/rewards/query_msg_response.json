{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsgResponse",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "claim_simulation"
      ],
      "properties": {
        "claim_simulation": {
          "$ref": "#/definitions/ClaimSimulationResult"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "account"
      ],
      "properties": {
        "account": {
          "$ref": "#/definitions/Account_for_HumanAddr"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "pool"
      ],
      "properties": {
        "pool": {
          "$ref": "#/definitions/RewardPool_for_HumanAddr"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "total_rewards_supply"
      ],
      "properties": {
        "total_rewards_supply": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "description": "Copy of SNIP20 message for Keplr support",
      "type": "object",
      "required": [
        "token_info"
      ],
      "properties": {
        "token_info": {
          "type": "object",
          "required": [
            "decimals",
            "name",
            "symbol"
          ],
          "properties": {
            "decimals": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            },
            "name": {
              "type": "string"
            },
            "symbol": {
              "type": "string"
            },
            "total_supply": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Uint128"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "description": "Copy of SNIP20 message for Keplr support",
      "type": "object",
      "required": [
        "balance"
      ],
      "properties": {
        "balance": {
          "type": "object",
          "required": [
            "amount"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Account_for_HumanAddr": {
      "description": "An account for a single user.",
      "type": "object",
      "required": [
        "claimed_at_snapshot",
        "last_claimed",
        "locked_amount",
        "owner"
      ],
      "properties": {
        "balance_history": {
          "description": "A history of any changes that occurred to `self.locked_amount` at a given snapshot index. It is reset upon claiming.",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Snapshot"
          }
        },
        "claimed_at_snapshot": {
          "description": "The snapshot that was last claimed at.",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "last_claimed": {
          "description": "The last time that the user claimed their rewards.",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "locked_amount": {
          "description": "The amount of LP tokens the owner has locked into this contract.",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        },
        "owner": {
          "description": "The owner of this account.",
          "allOf": [
            {
              "$ref": "#/definitions/HumanAddr"
            }
          ]
        },
        "pending_balances": {
          "description": "A list of submitted tokens that aren't included in the rewards calculations yet.",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/PendingBalance"
          }
        }
      }
    },
    "ClaimError": {
      "anyOf": [
        {
          "description": "Occurs when the rewards pool is currently empty.",
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "pool_empty"
              ]
            }
          }
        },
        {
          "description": "It is possible for the user's share to be so little, that the actual reward amount of rewards calculated to be zero. However, it is highly unlikely in practice.",
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "account_zero_reward"
              ]
            }
          }
        },
        {
          "description": "Occurs when the user tries to claim earlier than the designated claim interval.",
          "type": "object",
          "required": [
            "time_to_wait",
            "type"
          ],
          "properties": {
            "time_to_wait": {
              "description": "In Unix seconds.",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "type": {
              "type": "string",
              "enum": [
                "early_claim"
              ]
            }
          }
        }
      ]
    },
    "ClaimSimulationResult": {
      "type": "object",
      "required": [
        "actual_claimed",
        "reward_amount"
      ],
      "properties": {
        "actual_claimed": {
          "description": "The actual amount of rewards that would be claimed.",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        },
        "error": {
          "anyOf": [
            {
              "$ref": "#/definitions/ClaimError"
            },
            {
              "type": "null"
            }
          ]
        },
        "reward_amount": {
          "description": "The total amount of rewards that should be claimed.",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        }
      }
    },
    "ContractInstance_for_HumanAddr": {
      "description": "Info needed to talk to a contract instance.",
      "type": "object",
      "required": [
        "address",
        "code_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "code_hash": {
          "type": "string"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    },
    "PendingBalance": {
      "type": "object",
      "required": [
        "amount",
        "submitted_at"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "submitted_at": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "RewardPool_for_HumanAddr": {
      "description": "Represents a pair that is eligible for rewards.",
      "type": "object",
      "required": [
        "lp_token",
        "share",
        "size"
      ],
      "properties": {
        "lp_token": {
          "$ref": "#/definitions/ContractInstance_for_HumanAddr"
        },
        "share": {
          "description": "The reward amount allocated to this pool.",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        },
        "size": {
          "description": "Total amount locked by all participants.",
          "allOf": [
            {
              "$ref": "#/definitions/Uint128"
            }
          ]
        }
      }
    },
    "Snapshot": {
      "type": "object",
      "required": [
        "amount",
        "index"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "index": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
