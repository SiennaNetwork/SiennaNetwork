{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "properties": {
    "owner": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "register_tokens": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/TokenType_for_HumanAddr"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "TokenType_for_HumanAddr": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "custom_token"
          ],
          "properties": {
            "custom_token": {
              "type": "object",
              "required": [
                "contract_addr",
                "token_code_hash"
              ],
              "properties": {
                "contract_addr": {
                  "$ref": "#/definitions/HumanAddr"
                },
                "token_code_hash": {
                  "type": "string"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "native_token"
          ],
          "properties": {
            "native_token": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    }
  }
}
