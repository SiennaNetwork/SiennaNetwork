{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LPStakingInitMsg",
  "type": "object",
  "required": [
    "deadline",
    "inc_token",
    "master",
    "prng_seed",
    "reward_token",
    "token_info",
    "viewing_key"
  ],
  "properties": {
    "deadline": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "inc_token": {
      "$ref": "#/definitions/ContractInfo"
    },
    "master": {
      "$ref": "#/definitions/ContractInfo"
    },
    "prng_seed": {
      "$ref": "#/definitions/Binary"
    },
    "reward_token": {
      "$ref": "#/definitions/ContractInfo"
    },
    "token_info": {
      "$ref": "#/definitions/TokenInfo"
    },
    "viewing_key": {
      "type": "string"
    }
  },
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "ContractInfo": {
      "description": "Code hash and address of a contract.",
      "type": "object",
      "required": [
        "address",
        "code_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "code_hash": {
          "type": "string"
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    },
    "TokenInfo": {
      "type": "object",
      "required": [
        "name",
        "symbol"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        }
      }
    }
  }
}
